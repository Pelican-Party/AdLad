{
	"extends": "./.denoTypes/tsconfig.json",
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext", "WebWorker"],
		"target": "esnext",
		"checkJs": true,
		"module": "esnext",
		"strict": true,
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true
	},
	"include": [
		"src/**/*.js",
		"src/**/*.ts",
		"test",
		"scripts"
	]
}
